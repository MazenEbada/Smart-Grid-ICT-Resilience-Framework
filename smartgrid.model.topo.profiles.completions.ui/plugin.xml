<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension point="org.eclipse.ui.menus">
    <menuContribution locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
      <command commandId="smartgrid.model.topo.profiles.completions.execute"
        label="Execute all Completions" style="push">
        <visibleWhen
          checkEnabled="false">
   			<with variable="activeMenuSelection">
      		<iterate
          	 ifEmpty="false">
        	 <adapt type="org.eclipse.core.resources.IResource">
              <test property="org.eclipse.core.resources.extension" value="smartgridtopo" />
		         </adapt>
		      </iterate>
		   </with>
		</visibleWhen>
      </command>
    </menuContribution>
  </extension>
 <extension point="org.eclipse.ui.commands">
    <command
          defaultHandler="smartgrid.model.topo.profiles.completions.handler.CompletionExecuterHandler"
          description="Executes completions"
          id="smartgrid.model.topo.profiles.completions.execute"
          name="Execute completion">
    </command>
  </extension>

</plugin>