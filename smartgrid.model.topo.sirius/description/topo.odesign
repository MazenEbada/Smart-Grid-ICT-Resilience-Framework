            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="PowerConnection" sourceMapping="//@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='SmartMeter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@additionalLayers[name='PhysicalLayer']/@nodeMappings[name='NetworkNode'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='ControlCenter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='InterCom'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='GenericController']" targetMapping="//@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@additionalLayers[name='PowerLayer']/@nodeMappings[name='PowerGridNode']" targetFinderExpression="feature:ConnectedTo">
          <style sizeComputationExpression="3" foldingStyle="SOURCE" endsCentering="Both">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
          </style>
        </edgeMappings>
      </additionalLayers>
      <additionalLayers name="PhysicalLayer" activeByDefault="true">
        <nodeMappings name="NetworkNode" semanticCandidatesExpression="feature:ContainsNE" domainClass="smartgridtopo.NetworkNode">
          <style xsi:type="style:SquareDescription" borderSizeComputationExpression="3" showIcon="false" sizeComputationExpression="" width="2" height="2">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_blue']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="PhysicalConnection" semanticCandidatesExpression="feature:ContainsC" sourceMapping="//@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@additionalLayers[name='PhysicalLayer']/@nodeMappings[name='NetworkNode'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='SmartMeter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='ControlCenter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='InterCom'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='GenericController']" targetMapping="//@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@additionalLayers[name='PhysicalLayer']/@nodeMappings[name='NetworkNode'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='SmartMeter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='ControlCenter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='InterCom'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='GenericController']" targetFinderExpression="[Links->at(2)/]" sourceFinderExpression="[Links->at(1)/]" domainClass="smartgridtopo.PhysicalConnection" useDomainElement="true">
          <style targetArrow="NoDecoration" sizeComputationExpression="3" routingStyle="manhattan" endsCentering="Both">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
            <centerLabelStyleDescription showIcon="false" labelExpression="feature:name">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
      </additionalLayers>
      <additionalLayers name="LogicalLayer" activeByDefault="true">
        <edgeMappings name="LogicalConnection" semanticCandidatesExpression="feature:ContainsLC" sourceMapping="//@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@additionalLayers[name='PhysicalLayer']/@nodeMappings[name='NetworkNode'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='SmartMeter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='ControlCenter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='InterCom'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='GenericController']" targetMapping="//@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@additionalLayers[name='PhysicalLayer']/@nodeMappings[name='NetworkNode'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='SmartMeter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='ControlCenter'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='InterCom'] //@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='GenericController']" targetFinderExpression="[Links->at(2)/]" sourceFinderExpression="[Links->at(1)/]" domainClass="smartgridtopo.LogicalCommunication" useDomainElement="true">
          <style targetArrow="NoDecoration" sizeComputationExpression="3" endsCentering="Both">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
            <centerLabelStyleDescription showIcon="false" labelExpression="feature:name">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
      </additionalLayers>
    </ownedRepresentations>
  </ownedViewpoints>
  <ownedViewpoints name="Dump" modelFileExtension="sdfasdffdas">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="asdf" domainClass="smartgridtopo.SmartMeter">
      <defaultLayer name="Default"/>
    </ownedRepresentations>
  </ownedViewpoints>
</description:Group>
