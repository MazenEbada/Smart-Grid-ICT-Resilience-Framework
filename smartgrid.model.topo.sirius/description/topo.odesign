            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_orange']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='orange']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="InterCom" semanticCandidatesExpression="feature:ContainsNE" domainClass="smartgridtopo.InterCom">
          <style xsi:type="style:EllipseNodeDescription" borderSizeComputationExpression="3" showIcon="false" sizeComputationExpression="0" horizontalDiameterComputationExpression="3" verticalDiameterComputationExpression="3">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_green']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
          </style>
        </nodeMappings>
        <toolSections name="Entities">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="SmartMeter" nodeMappings="//@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@defaultLayer/@nodeMappings[name='SmartMeter']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="smartgridtopo.SmartMeter" referenceName="ContainsNE"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="PhysicalConnection" edgeMappings="//@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@additionalLayers[name='PhysicalLayer']/@edgeMappings[name='PhysicalConnection']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="[target.eContainer()/]">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="smartgridtopo.PhysicalConnection" referenceName="ContainsC">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="Links" valueExpression="var:target"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="Links" valueExpression="var:source"/>
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:source">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="LinkedBy" valueExpression="var:instance"/>
                  </subModelOperations>
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:target">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="LinkedBy" valueExpression="var:instance"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="PowerConnection" edgeMappings="//@ownedViewpoints[name='Topology']/@ownedRepresentations[name='SmartGridTopology']/@additionalLayers[name='PowerLayer']/@edgeMappings[name='PowerConnection']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:source">
                <subModelOperations xsi:type="tool_1:SetValue" featureName="ConnectedTo" valueExpression="var:target"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
